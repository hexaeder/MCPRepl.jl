{
  "mcpServers": {
    "julia-repl": {
      "type": "streamable-http",
      "url": "http://localhost:{{port}}",
{% if has_api_key %}
      "headers": {
        "Authorization": "{{api_key}}"
      },
{% end %}
      "alwaysAllow": [
                  {{tool_list}}
      ],
      "disabled": false
    }
  }
}